{
  "name": "turf-isolines",
  "version": "1.0.3",
  "description": "turf isolines module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js test.js",
    "pretest": "npm run lint",
    "test": "tape test.js",
    "doc": "dox -r < index.js | doxme --readme > README.md"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": [
        1
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Turfjs/turf-isolines.git"
  },
  "keywords": [
    "turf",
    "geojson",
    "isolines",
    "contours",
    "elevation",
    "topography"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf-isolines/issues"
  },
  "homepage": "https://github.com/Turfjs/turf-isolines",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "dox": "^0.6.1",
    "doxme": "^1.4.3",
    "eslint": "^1.10.3",
    "eslint-config-mourner": "^1.0.1",
    "tape": "^3.5.0"
  },
  "dependencies": {
    "turf-extent": "^1.0.3",
    "turf-featurecollection": "^1.0.0",
    "turf-grid": "^1.0.0",
    "turf-inside": "^1.1.3",
    "turf-linestring": "^1.0.1",
    "turf-planepoint": "^1.0.0",
    "turf-square": "^1.0.0",
    "turf-tin": "^1.0.5"
  }
}
